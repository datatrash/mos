{
  "name": "mos",
  "publisher": "datatra.sh",
  "displayName": "mos",
  "description": "MOS 6502 extension",
  "version": "0.0.1",
  "preview": true,
  "license": "MIT",
  "repository": {
    "url": "https://github.com/datatrash/mos.git",
    "type": "git"
  },
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/mos.toml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "asm",
        "extensions": [
          ".asm"
        ],
        "aliases": [
          "6502 Assembly"
        ],
        "configuration": "language-configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "dependencies": {
    "vscode-languageclient": "^7.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "typescript": "^4.1.3"
  }
}
