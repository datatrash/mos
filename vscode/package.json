{
  "name": "mos",
  "publisher": "datatrash",
  "displayName": "mos",
  "description": "An extension for the MOS 6502 assembler.",
  "categories": [
    "Programming Languages",
    "Formatters"
  ],
  "version": "0.0.1",
  "preview": true,
  "license": "MIT",
  "homepage": "README.md",
  "galleryBanner": {
    "color": "#668",
    "theme": "dark"
  },
  "icon": "icon.png",
  "bugs": {
    "url": "https://github.com/datatrash/mos/issues"
  },
  "repository": {
    "url": "https://github.com/datatrash/mos.git",
    "type": "git"
  },
  "engines": {
    "vscode": "^1.53.0"
  },
  "activationEvents": [
    "workspaceContains:**/mos.toml"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "asm",
        "extensions": [
          ".asm"
        ],
        "aliases": [
          "6502 Assembly"
        ],
        "configuration": "language-configuration.json"
      }
    ],
    "configuration": {
      "title": "MOS",
      "properties": {
        "mos.path": {
          "type": "string",
          "description": "Path to the mos executable",
          "markdownDescription": "Path to the `mos` executable"
        }
      }
    }
  },
  "scripts": {
    "publish": "vsce publish",
    "mos-version": "node mos-version",
    "vscode:prepublish": "npm run mos-version && webpack --mode production",
    "compile": "webpack --mode development"
  },
  "dependencies": {
    "@types/decompress": "^4.2.3",
    "@types/node-fetch": "^2.5.8",
    "@types/rimraf": "^3.0.0",
    "decompress": "^4.2.1",
    "https-proxy-agent": "^5.0.0",
    "node-fetch": "^2.6.1",
    "rimraf": "^3.0.2",
    "vscode-languageclient": "^7.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^14.14.32",
    "@types/vscode": "^1.53.0",
    "glob": "^7.1.6",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.3",
    "vsce": "^1.85.1",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.5.0"
  }
}
